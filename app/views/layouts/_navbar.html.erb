<%= navbar brand: 'Reading Bowl', brand_link: '/', class: 'navbar navbar-inverse', fixed: :top, responsive: true do %>
  <% current_user = user %>
  <% if current_user.present? %>
    <% if current_user.instructor? %>
      <%= render 'layouts/instructor_navbar' %>
    <% else %>
      <%= render 'layouts/student_navbar' %>
    <% end %>
  <% else %>
    <%= render :partial => 'layouts/logged_out_navbar' %>
  <% end %>
<% end %>
